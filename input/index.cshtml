Order: 0
ArchiveSources: => PostSources
ArchiveDestination: >
  => GetInt("Index") <= 1 ? $"index.html" : $"page/{GetInt("Index")}.html"
ArchivePageSize: 3
ArchiveOrderKey: Published
ArchiveOrderDescending: true
ArchiveTitle: >
  =>
  {
    string title = GetString("Title");
    if (GetInt("Index") > 1)
    {
      title = string.IsNullOrEmpty(title) ? ("Page " + GetInt("Index")) : (title + " (" + "Page " + GetInt("Index") + ")");
    }
    return title;
  }
---
@Html.Partial("/_posts.cshtml", Document)